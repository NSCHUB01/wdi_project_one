<h3>Log in</h3>

<%= form_tag "sessions" do %>
  <ul>
    <li><%= label_tag :email %><br /></li>
    <li><%= text_field_tag :email, params[:email] %></li>
    <li><%= label_tag :password %><br /></li>
    <li><%= password_field_tag :password %></li>
  </ul>
  <div class="actions"><%= submit_tag "Log in" %></div>
<% end %>
